        "tslint": "5.11.0",
        "tslint-config-prettier": "1.16.0",
        "tslint-loader": "3.6.0",
        "typescript": "3.1.6",
        "url-loader": "2.2.0",
        "webpack": "4.26.0",
        "webpack-cli": "3.1.2",
